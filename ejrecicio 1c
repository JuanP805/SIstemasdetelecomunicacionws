void rellenar_NombreCliente() {
    for (int i = 0; i < 195; i++) {
        volatile auto x = nombres[i];
    }
}

void imprimir_NombreCliente() {
    for (int i = 0; i < 195; i++) {
        cout << nombres[i] << endl;
    }
}

int main() {
    cout << "Ejercicio 1 inciso c" << endl;

    auto inicio = chrono::high_resolution_clock::now();
    rellenar_NombreCliente();
    auto medio = chrono::high_resolution_clock::now();

    imprimir_NombreCliente();
    auto fin = chrono::high_resolution_clock::now();

    auto duracion1 = chrono::duration_cast<chrono::microseconds>(medio - inicio);
    auto duracion2 = chrono::duration_cast<chrono::microseconds>(fin - medio);

    cout << "Tiempo de Generacion de Nombres: " << duracion1.count() << " microsegundos" << endl;
    cout << "Tiempo de Impresion de Nombres: " << duracion2.count() << " microsegundos" << endl;

    return 0;
}
